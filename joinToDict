def inputString():
	return input()
#def inputEquation(self):
#	return input()
def convertToLOT(string):
	lis = [tuple(i.split('=')) for i in string.split(';')];
	return lis
def convertToLOD(lis):	
	li = [{a[0]:a[1]} for a in lis]
	return li
def convertToD(lis):
	dic = {a[0]:a[1] for a in lis}
	return dic

def printL(li):
	print(li)

def main():
	#string = "a=b;c=d;e=f"
	print("Enter the string")
	string = inputString()
	li = convertToLOT(string)
	printL(li)
	lis = convertToLOD(li)
	printL(lis)
	dic = convertToD(li)
	printL(dic)

if __name__ == "__main__":
	main()
